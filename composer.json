{
    "name": "apdevs/laravel-utils",
    "type": "library",
    "version": "0.2.0",
    "require": {
        "illuminate/support": "~5|~6|~7|~8|~9|~10",
    },
    "license": "MIT",
    "authors": [
        {
            "name": "JManuel Ruiz",
            "email": "jmanuel@apdevs.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "APDevs\\LaravelUtils\\LaravelUtilsServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "APDevs\\LaravelUtils\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
